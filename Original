         driverState = scnr.nextLine();

      break;
    
    }
    
      //  -Exceptions for timeUnit entries
      //  -For statements for time unit delinieations
      
      
   //Federal tax rate
 
      if(filingStatus.equals("n")){
         
         if (taxableIncome < 9225){
         
          fedTaxRate = .1;  
            
         }else if (taxableIncome > 9225 && taxableIncome < 37450){
            
            fedTaxRate = .015;
            
         }else if(taxableIncome > 37450 && taxableIncome < 90750){
            
            fedTaxRate = .25;
         
         }else if(taxableIncome > 90750 && taxableIncome < 189300){
            
            fedTaxRate = .28;
         
         }else if(taxableIncome > 189300 && taxableIncome < 411500){
         
            fedTaxRate = .33;
         
         }else if(taxableIncome > 411500 && taxableIncome < 413200){
         
            fedTaxRate = .35;
            
         }else if(taxableIncome > 413200){
            
            fedTaxRate = .396;
         
         }
           
      
      }
      
         taxRate = taxRate + fedTaxRate + selfEmploymentRate;   
      
   //Calculate expenses
      
expenses = ((grossEarnings * taxRate) + ((milesDriven / carMpg) * gasPrice * fillUps) + insurance + maintenanceAmount - (milesDriven * mileageReductionRate) + miscellaneousExpenses);
      
   //Calculate realEarnings
      
         realEarnings = (grossEarnings - expenses); 
         
  //Calculate Earning/timeUnit
  //Initialize String array
  //Separate period into a numericUnit and Unit of Time
      
      String [] strSplit = new String[1];  
      
      strSplit = period.split("-");
      
  //Convert the numericUnit value to int type
  //Store numericUnit & timeUnit values in variables 
     
     numericUnit = Integer.parseInt(strSplit[0]);
     
     timeUnit = strSplit [1]; 
     
   //Change timeUnit from plural to singular
     
    if (timeUnit.equals("days")){
      
        char s = 's';
        
         if( timeUnit.charAt(3) == s){
         
               
              timeUnit = newTimeUnit;
              
              newTimeUnit = "day";
      
      }
        
         }else if (timeUnit.equals("weeks")){
      
         char s = 's';
         
         if(timeUnit.charAt(4) == s){
         
               
              timeUnit = newTimeUnit;
              
              newTimeUnit = "week";
      
      }
  
            }else if (timeUnit.equals("years")){
      
            char s = 's';

            if(timeUnit.charAt(4) == s){
        
               
               timeUnit = newTimeUnit;

               newTimeUnit = "year";
      
      }

             } 
      
   //Print out stats
      
      System.out.printf("Real Earnings: %.2f", realEarnings);
      System.out.printf("\nTotal expenses: %.2f", expenses);
      System.out.printf("\nTotal taxes owed: %.2f", (grossEarnings * taxRate));
      System.out.printf("\nTotal spent on gas: %.2f", (fillUps * gasPrice));
      System.out.printf("\nEarnings per %s: %.2f", newTimeUnit, (realEarnings/numericUnit));
      System.out.printf("\n%.2f per hr", (grossEarnings / hoursWorked));
        
   }
}




